import numpy as np
import pandas as pd

data = np.genfromtxt('input.txt',dtype=int, delimiter = '\t')

# part 1
count = 0
for i in range (0,len(data)):
    if i==0:
        count == 0
    elif data[i]>data[i-1]:
        count = count+1

print(count)

# part 2
count = 0
for i in range(3,len(data)):
    if data[i-3]<data[i]:
        count=count+1

print(count)
