import numpy as np
import collections

######### DATA PREPARATION
data = np.genfromtxt('input11',dtype=str, delimiter = '\t')

mylist = []
for i in range(len(data)):
  result = []
  for digit in data[i]:
    result.append(int(digit))
  mylist.append(result)

print(mylist)




for i in range(1,101):

  for j in range(0,range(len(mylist))):
    mylist[j] = [x+1 for x in mylist[j]]

